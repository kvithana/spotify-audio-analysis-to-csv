{
    "compilerOptions": {
        "outDir": "build",
        "module": "commonjs",
        "target": "es5",
        "lib": [
            "es2019"
        ],
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "noImplicitAny": true,
        "sourceMap": true,
        "esModuleInterop": true
    },
    "include": [
        "src",
    ],
    "types": [
        "node"
    ],
    "typeRoots": [
        "node_modules/@types"
    ]
}